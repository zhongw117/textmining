    "def multiple_replace(text, idict):\n",
    "    rx = re.compile('|'.join(map(re.escape, idict)))\n",
    "\n",
    "    def one_xlat(match):\n",
    "        return idict[match.group(0)]\n",
    "\n",
    "    return rx.sub(one_xlat, text)\n"
   ]
